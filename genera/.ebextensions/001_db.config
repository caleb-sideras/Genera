commands:
    setvars:
        command: /opt/elasticbeanstalk/bin/get-config environment | jq -r 'to_entries | .[] | "export \(.key)=\"\(.value)\""' > /etc/profile.d/sh.local
        leader_only: true
    make_sure_command_is_run:
        command: source /etc/profile.d/sh.local
        leader_only: true
        
packages:
    yum:
        mysql-devel: []