container_commands:
    01_migrate_all:
        command: source $PYTHONPATH/activate && makemigrations genera && makemigrations main && makemigrations payments
        leader_only: true
    02_migrate_final:
        command: source $PYTHONPATH/activate && python manage.py migrate --run-syncdb && python manage.py makemigrations && python populate.py
        leader_only: true